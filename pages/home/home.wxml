<!--1. 小程序数据绑定 -->

<!-- <view>Hello CoderWhy</view>   {{}} ->Mustache语法  -->

<view>Hello {{name}}</view>
<view>我的年龄: {{age}}</view>

<!-- 2.列表展示  wx:key  wx:for   -->
